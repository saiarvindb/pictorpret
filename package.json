{
	"name" : "pictorpret",
	"version" : "1.0.0",
	"description" : "Translate text in pictures while browsing the web.",
	"author" : "Sai Arvind Budaraju",
	"scripts" :
	{
		"dev" : "rm -rf dist/ && webpack -w",
		"start" : "web-ext run -s dist/",
		"build" : "rm -rf dist/ && webpack && web-ext build -s dist/ -a dist/ext/",
		"clean" : "rm -rf dist/ node_modules/"
	},
	"dependencies" :
	{
		"web-ext" : "7.8.0",
		"webpack": "5.88.2",
		"html-webpack-plugin" : "5.5.3",
		"copy-webpack-plugin" : "11.0.0",
		"webpack-cli" : "5.1.4",
		"typescript" : "5.2.2",
		"ts-node" : "10.9.1",
		"react" : "18.2.0",
		"@types/react" : "18.2.25",
		"react-dom" : "18.2.0",
		"@types/react-dom" : "18.2.10",
		"ts-loader" : "9.4.4",
		"tesseract.js" : "5.0.2",
		"webextension-polyfill" : "0.10.0",
		"@types/webextension-polyfill" : "0.10.4",
		"@mui/material": "5.14.13",
		"@emotion/react": "11.11.1",
		"@emotion/styled": "11.11.0",
		"axios": "1.6.0"
	}
}
